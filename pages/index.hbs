{{#> body}}

<div class="panel">
	<div class="panel_body">
		<h1 class="h1">
			{{practices_title}}
			<span class="heading-border"></span>
		</h1>
		<div class="wedge-3x"></div>
		<p>{{practices_beskrivelse}}</p>
		<div class="wedge-3x"></div>
		<div class="practices">
			{{#practices}} {{#each this}}
			<a href="/practices/{{page_slug}}" class="practice">
				<div class="practice__image" style="background-image: url('{{practice_entry.image}}')"></div>
				<div class="practice__info">
					<div class="practice__name">{{practice_entry.title}}</div>
				</div>
				<div class="practice__desc contentbox contentbox--full-width">
					{{practice_entry.description}}
				</div>
			</a>
			{{/each}} {{/practices}}
		</div>
	</div>
</div>

<div class="wedge-3x"></div>

<div class="panel" style="background-color:{{price_background_color}}">
	<div class="panel_body fullwidth">
		<div class="sp-header">
			<div class="contentbox_content">
				<h1 class="h1">{{price_title}}</h1>
				<h2 class="h2">{{price_subtitle}}</h2>
			</div>
		</div>
	</div>

	<div class="panel_body">
		<div class="cards">
			{{price_sdescription}} {{#each price_list}}
			<div class="card">
				<div class="card__wrap">
					<div class="card__title">
						<h3 class="h3">{{title}}</h3>
					</div>
					<div class="card__content">
						{{description}}
					</div>
					<div class="card__bottom">
						<div class="card__info">{{price}}</div>
						<div class="card__info">{{meta_info}}</div>
					</div>
				</div>
			</div>
			{{/each}}
		</div>
	</div>
</div>

<div class="panel color_3 text_center">
	<div class="panel_body">
		<h2 class="h2">{{cta_tagline}}</h2>
		<div class="wedge"></div>
		<h3 class="h3">{{cta_text}}</h3>
		<div class="wedge-3x"></div>
		<a class="btn" href="mailto:{{cta_mail}}">{{cta_button_text}}</a>
	</div>
</div>

{{/body}}